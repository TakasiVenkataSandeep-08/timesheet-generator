{
    "name": "timesheet-generator",
    "version": "0.0.1",
    "description": "Best-in-class automated timesheet generation from git commits with intelligent time estimation",
    "type": "commonjs",
    "main": "./src/index.js",
    "exports": {
        ".": {
            "require": "./src/index.js",
            "import": "./src/index.js",
            "default": "./src/index.js"
        },
        "./package.json": "./package.json"
    },
    "bin": {
        "timesheet": "./bin/timesheet"
    },
    "files": [
        "src/",
        "bin/",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        ".timesheetrc.example"
    ],
    "scripts": {
        "start": "node ./bin/timesheet",
        "start-cli": "node ./bin/timesheet",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --maxWorkers=2",
        "prepublishOnly": "npm run test:ci",
        "prepare": "node -e \"require('fs').existsSync('.git/hooks') || require('child_process').execSync('git init', {stdio:'ignore'})\"",
        "version": "node -e \"console.log(require('./package.json').version)\""
    },
    "keywords": [
        "git",
        "commits",
        "timesheet",
        "time-tracking",
        "time-tracking-automation",
        "automation",
        "jira",
        "linear",
        "toggl",
        "clockify",
        "vcs",
        "github",
        "gitlab",
        "git-commits",
        "work-log",
        "timesheet-generator",
        "timesheet-generator-cli",
        "timesheet-generator-mcp-server",
        "mcp-server",
        "model-context-protocol"
    ],
    "author": {
        "name": "Takasi Venkata Sandeep",
        "email": "takasivenkatasandeep@gmail.com"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TakasiVenkataSandeep-08/timesheet-generator.git",
        "directory": "."
    },
    "bugs": {
        "url": "https://github.com/TakasiVenkataSandeep-08/timesheet-generator/issues"
    },
    "homepage": "https://github.com/TakasiVenkataSandeep-08/timesheet-generator#readme",
    "dependencies": {
        "@linear/sdk": "^70.0.0",
        "@modelcontextprotocol/sdk": "^1.25.2",
        "chart.js": "^4.5.1",
        "commander": "^11.1.0",
        "date-holidays": "^3.26.6",
        "inquirer": "^13.2.0",
        "ora": "^9.0.0",
        "pdfkit": "^0.17.2",
        "toggl-api": "^1.0.2"
    },
    "devDependencies": {
        "@jest/globals": "^30.2.0",
        "jest": "^30.2.0"
    },
    "sideEffects": false
}